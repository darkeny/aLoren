datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
enum Sexo {
  MALE
  FEMALE
}

enum Role {
  USER
  ADMIN
}

model User {
  id Int @id @default(autoincrement())
  name String
  sexo Sexo
  email String @unique
  role Role @default(USER)
  
}


